// Define o pino onde o LED está conectado
const int ledPin = 13;

void setup() {
  // Configura o pino do LED como saída
  pinMode(ledPin, OUTPUT);
}

void loop() {
  // Liga o LED
  digitalWrite(ledPin, HIGH);
  
  // Atraso de 1 segundo (1000 milissegundos)
  delay(1000);
  
  // Desliga o LED
  digitalWrite(ledPin, LOW);
  
  // Atraso de 1 segundo (1000 milissegundos)
  delay(1000);
}
